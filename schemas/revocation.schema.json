{
  "type": "object",
  "properties": {
    "schema_version": {
      "type": "string",
      "enum": [
        "1.0"
      ]
    },
    "sequence_number": {
      "type": "integer",
      "exclusiveMinimum": 0
    },
    "issued_at": {
      "type": "string",
      "format": "date-time"
    },
    "expires_at": {
      "type": "string",
      "format": "date-time"
    },
    "next_update": {
      "type": "string",
      "format": "date-time"
    },
    "entries": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "minLength": 1
          },
          "versions": {
            "type": "array",
            "items": {
              "type": "string",
              "minLength": 1
            },
            "minItems": 1
          },
          "revoked_at": {
            "type": "string",
            "format": "date-time"
          },
          "reason": {
            "type": "string",
            "minLength": 1
          },
          "severity": {
            "type": "string",
            "minLength": 1
          }
        },
        "required": [
          "name",
          "versions",
          "revoked_at",
          "reason",
          "severity"
        ],
        "additionalProperties": false
      }
    },
    "signature": {
      "type": "object",
      "properties": {
        "keyid": {
          "type": "string",
          "minLength": 1
        },
        "sig": {
          "type": "string",
          "minLength": 1
        }
      },
      "required": [
        "keyid",
        "sig"
      ],
      "additionalProperties": false
    }
  },
  "required": [
    "schema_version",
    "sequence_number",
    "issued_at",
    "expires_at",
    "next_update",
    "entries",
    "signature"
  ],
  "additionalProperties": false,
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://haldir.ai/schemas/revocation.schema.json",
  "title": "ASAF revocation"
}
